<p id="notice"><%= notice %></p>

<h1>Configurações de estilo</h1>

<table>
  <thead>
    <tr>
      <th>Background image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @styles_configs.each do |styles_config| %>
      <tr>
        <td><%= styles_config.background_image_url %></td>
        <td><%= link_to 'Show', styles_config %></td>
        <td><%= link_to 'Edit', edit_styles_config_path(styles_config) %></td>
        <td><%= link_to 'Destroy', styles_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Styles Config', new_styles_config_path %>
